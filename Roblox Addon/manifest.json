{
  "manifest_version": 3,
  "name": "Roblox Addon",
  "version": "1.0",
  "description": "Imports custom css to the Roblox website.",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.roblox.com/*"],
      "js": ["JavaScript.js"],
      "css": ["Style.css"]
    }
  ]
}